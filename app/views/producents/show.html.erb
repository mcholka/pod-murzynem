<% title "Podglad" %>

<% if notice -%>
<div id="notice"><%= notice %></div>
<% end -%>

<article class="single">
  <div class="attribute">
    <span class="name">Nazwa</span>
    <span class="value"><%= @producent.nazwa %></span>
  </div>
  <div class="attribute">
    <span class="name">Link</span>
    <span class="value"><%= @producent.link %></span>
  </div>
  <div class="link">
    <%= link_to 'Edytuj', edit_producent_path(@producent) %> |
    <%= link_to 'Strona Glowna', producents_path %>
  </div>
</article>
