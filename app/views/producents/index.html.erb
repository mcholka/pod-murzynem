<% title "Producenci" %>

<% @producents.each do |producent| %>
<article class="index">
  <div class="attribute">
  <img src="<%= producent.link %>" height="375" width="500" />
  <% @id = producent.id %>
  <%= link_to '->', telefons_path(producent_id: @id.to_i) %>
  </div>
</article>
<% end %>
<%= paginate @producents %>

