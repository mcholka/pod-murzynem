<% title "Producenci" %>

<% @producents.each do |producent| %>
<article class="index">
  <div class="attribute">
  <% @id = producent.id %>
  <a href="<%=telefons_path(producent_id: @id.to_i) %>"><img src="<%= producent.link %>" height="375" width="500" /></a>
  </div>


<% if admin %>
<div id="user_nav">
   <div class="links">
        <%= link_to 'Usun', producent, confirm: 'Napewno wywaliÄ‡??', method: :delete %>
  </div>
  <% end %>

</article>
<% end %>
<%= paginate @producents %>

